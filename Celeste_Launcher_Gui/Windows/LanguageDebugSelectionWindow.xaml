<Window x:Class="Celeste_Launcher_Gui.Windows.LanguageDebugSelectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Celeste_Launcher_Gui.Windows"
        mc:Ignorable="d"
        Title="LanguageDebugSelectionWindow" Height="200" Width="200" Topmost="True">
    <Grid>
        <StackPanel>
            <Button Content="German" Click="SetGerman"/>
            <Button Content="English" Click="SetEnglish"/>
            <Button Content="Spanish" Click="SetSpanish"/>
            <Button Content="French" Click="SetFrench"/>
            <Button Content="Italian" Click="SetItalian"/>
            <Button Content="Chinese" Click="SetChinese"/>
            <Button Content="Brazilian" Click="SetBrazilian"/>
        </StackPanel>
    </Grid>
</Window>
